/******************************公用弹窗css***************************/
.reveal-modal-bg {
	position: fixed;
	height: 100%;
	width: 100%;
	background: #000;
	background: rgba(0, 0, 0, .3);
	z-index: 100;
	display: none;
	top: 0;
	left: 0;
	font-family: "Microsoft YaHei", "Helvetica Neue", Helvetica, Arial, sans-serif;
}
.reveal-modal {
	visibility: hidden;
	top: 120px;
	left: 50%;
	margin-left: -183px;
	width: 366px;
	background: rgb(255, 255, 255);
	position: absolute;
	z-index: 101;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	-moz-box-shadow: 0 0 10px rgba(0, 0, 0, .4);
	-webkit-box-shadow: 0 0 10px rgba(0, 0, 0, .4);
	-box-shadow: 0 0 10px rgba(0, 0, 0, .4);
}
.reveal-modal.small {
	width: 200px;
	margin-left: -100px;
}
.reveal-modal.medium {
	width: 400px;
	margin-left: -200px;
}
.reveal-modal.large {
	width: 600px;
	margin-left: -300px;
}
.reveal-modal.xlarge {
	width: 800px;
	margin-left: -440px;
}
/*弹窗公用标题样式*/
.reveal-modal .d-title {
	font-size: 14px;
	color: #333;
	height: 40px;
	line-height: 40px;
	font-weight: bold;
	border-bottom: 1px solid #ddd;
	padding-left: 15px;
	margin: 0;
	position: relative;
}
/*右上角结束标志*/
.reveal-modal .close-reveal-modal {
	font-size: 30px;
	line-height: .5;
	position: absolute;
	top: 10px;
	right: 8px;
	color: #aaa;
	text-shadow: 0 -1px 1px rbga(0, 0, 0, .6);
	font-weight: bold;
	cursor: pointer;
}
.reveal-modal .close-reveal-modal:hover {
	color: #666;
}






/********************************公用弹窗结束**************************/




